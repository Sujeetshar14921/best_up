{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "dist/assets/$1"
    },
    {
      "src": "/(.*\\.js)",
      "dest": "dist/$1"
    },
    {
      "src": "/(.*\\.css)",
      "dest": "dist/$1"
    },
    {
      "src": "/(.*\\.svg)",
      "dest": "dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.html"
    }
  ]
}
