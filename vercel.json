{
  "version": 2,
  "buildCommand": "npm run build:all",
  "outputDirectory": "frontend/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/server.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "admin_dashboard/dist/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/dist/index.html"
    }
  ]
}
